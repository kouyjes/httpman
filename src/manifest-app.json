{
  "manifest_version": 2,
  "name": "__MSG_CHROME_HANGOUTS_SHORT_NAME__",
  "description": "httpman ",
  "version": "1.0",
  "app": {
    "background": {
      "scripts": ["index-app.js"]
    }
  },
  "default_locale": "en",

  "icons": {
    "128": "images/icon.png",
    "48": "images/icon.png"
  },
  "sandbox":{
    "pages": ["html/panel.html"],
    "content_security_policy1":"sandbox allow-scripts script-src 'self' unsafe-inline unsafe-eval child-src allow-forms allow-popups allow-modals"
  }
}